<div class="build-head">
    <div class="build-head-title">All CPU Coolers:</div>
    <a class="build-head-return" href="/"> <= Return to Home Page</a>
</div>
<a href="/cpucooler/new">
    <button>
        Add New CPU Cooler
    </button>
</a>

{{#each allCpuCoolers}}
    <a href="/cpucooler/{{_id}}">
        <div class="list-item">
            {{name}}
            <div class ="item">
                <div class="title">Manufacturer:</div>
                {{brand}}
            </div>
            <div class ="item">
                <div class="title">Cooler Design:</div>
                {{type}}
            </div>
        </div>
    </a>
{{/each}}
