<div class="build-head">
    <div class="build-head-title">All GPU Coolers:</div>
    <a class="build-head-return" href="/"> <= Return to Home Page</a>
</div>
<a href="/gpucooler/new">
    <button>
        Add New GPU Cooler
    </button>
</a>

{{#each allGpuCoolers}}
    <a href="/gpucooler/{{_id}}">
        <div class="list-item">
            {{name}}
            <div class ="item">
                <div class="title">Manufacturer:</div>
                {{brand}}
            </div>
            <div class ="item">
                <div class="title">Cooler Design:</div>
                {{type}}
            </div>
        </div>
    </a>
{{/each}}
