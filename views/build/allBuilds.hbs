<h1>All PC Desktop Builds:</h1>

<a href="/build/new">
    <button>
        Configure Your Own Build
    </button>
</a>

{{#each allBuilds}}
    <a href="/build/{{_id}}">
        <div class="build-item">
            <div class ="item">
                {{name}}
            </div>
            <div class ="item">
                <div class="title">Date Created:</div>
                {{date}}
            </div>
            </div>
        </div>
    </a>
{{/each}}
