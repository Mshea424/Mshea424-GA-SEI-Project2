
{{singleBuild.name}}

<a href="/build">
    <button>Return to List of All Builds</button>
</a>

<a href="/build/{{singleBuild._id}}/edit">
    <button>Update This Build</button>
</a>

<form action="/build/{{singleBuild._id}}?_method=DELETE" method="POST">
    <input type="submit" value="Delete this Build">
</form>

